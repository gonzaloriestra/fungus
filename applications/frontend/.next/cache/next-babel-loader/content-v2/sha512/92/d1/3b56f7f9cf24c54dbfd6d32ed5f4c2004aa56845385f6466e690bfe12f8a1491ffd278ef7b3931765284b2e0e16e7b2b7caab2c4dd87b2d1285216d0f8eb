/withAuthRequired';\n\nimport Location from './models/Location';\n\nimport Item from './components/Item';\nimport Header, { ActivePage } from '../../components/Header';\nimport Map from './components/Map';\nimport addLocation from './queries/addLocation';\n\nconst handleOnSubmit = async () => {\n  await addLocation({ name, zone });\n\n  router.push('/locations');\n};\n\nfunction ModalExampleModal({ show }) {\n  const [open, setOpen] = React.useState(show);\n\n  useEffect(() => {\n    setOpen(show);\n  }, [show]);\n\n  return (\n    <Modal onClose={() => setOpen(false)} onOpen={() => setOpen(true)} open={open}>\n      <Modal.Header>Nueva zona</Modal.Header>\n      <Modal.Content image>\n        <Modal.Description>\n          <Form onSubmit={handleOnSubmit}>\n            <Form.Field>\n              <div\n                style={{\n                  height: '500px',\n                }}\n              >\n                <Map mode=\"edit\" initialZoom={6} onLocationCreated={(zone): void => setZone(zone)} />\n              </div>\n            </Form.Field>\n            <Form.Field>\n              <label>¿Qué nombre quieres darle?</label>\n              <input placeholder=\"Escribe un nombre\" onChange={(e): void => setName(e.target.value)} />\n            </Form.Field>\n          </Form>\n        </Modal.Description>\n      </Modal.Content>\n      <Modal.Actions>\n        <Button color=\"black\" onClick={() => setOpen(false)}>\n          Cancelar\n        </Button>\n        <Button primary onClick={() => setOpen(false)} positive>\n          Guardar\n        </Button>\n      </Modal.Actions>\n    </Modal>\n  );\n}\n\nfunction Locations(): JSX.Element {\n  const result = useSWR(`/api/me/locations`, fetcher);\n  const [showAddLocationModal, setShowAddLocationModal] = React.useState(false);\n\n  const locations: Array<Location> = result.data;\n  const error: Error = result.error;\n\n  if (error) {\n    return <div>Loading failed: {error.message}</div>;\n  }\n\n  if (!locations